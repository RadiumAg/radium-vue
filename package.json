{
  "name": "radium-vue",
  "version": "1.0.4",
  "description": "A Component Library for Vue3.0",
  "main": "lib/index.js",
  "scripts": {
    "gen": "bash ./scripts/gc.sh",
    "build": "lerna bootstrap && npm run clean:lib && node build/build.js && npm run build:theme",
    "build:util": "node ./build/scriptjs/build-utils.js",
    "build:theme": "gulp build --gulpfile ./packages/theme-chalk/gulpfile.js && cp-cli ./packages/theme-chalk/lib lib/theme-chalk && rimraf packages/theme-chalk/lib",
    "clean:lib": "rimraf lib && rimraf es",
    "website-build": "cross-env NODE_ENV=production webpack --config ./website/webpack.config.js",
    "website-dev": "cross-env NODE_ENV=development webpack-dev-server --config ./website/webpack.config.js",
    "website-bundle": "cross-env NODE_ENV=development MODE=bundle webpack-dev-server --config ./website/webpack.config.js",
    "upgrade:vue": "bash ./scripts/update-versions.sh 3.2.33",
    "clear-node_module": "rimraf node_modules",
    "npm-i-legacy": " npm i --legacy-peer-deps",
    "eslint-fix": "eslint ./**  --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/Agrement/radium-vue.git"
  },
  "author": "Radium",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/component-compiler-utils": "^3.3.0",
    "algoliasearch": "^4.15.0",
    "chalk": "^5.2.0",
    "clipboard-copy": "^4.0.1",
    "components-helper": "^2.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "file-loader": "^6.2.0",
    "file-save": "^0.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-rename": "^2.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "import-from": "^4.0.0",
    "klaw-sync": "^6.0.0",
    "lerna": "^6.5.1",
    "lint-staged": "^13.1.2",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "mini-css-extract-plugin": "^2.7.2",
    "mitt": "^3.0.0",
    "normalize.css": "^8.0.1",
    "ora": "^6.1.2",
    "prettier": "2.8.4",
    "rimraf": "^4.3.0",
    "rollup": "^3.18.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.58.3",
    "throttle-debounce": "5.0.0",
    "ts-morph": "^17.0.1",
    "typescript": "^4.9.5",
    "vue": "^3.2.47"
  },
  "dependencies": {
    "@radium-vue/components": "*",
    "@radium-vue/utils": "*",
    "@popperjs/core": "^2.11.6",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "resize-observer-polyfill": "^1.5.1"
  },
  "files": [
    "lib",
    "packages",
    "es"
  ]
}

{
  "compilerOptions": {
    "module": "ESNext",
    "noImplicitAny": false,
    "removeComments": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "noLib": false,
    "target": "es6",
    "sourceMap": true,
    "baseUrl":".",
    "paths": {
      "Pages/*": ["website/src/pages/*"],
      "Docs/*":["website/src/docs"],
      "Core/*":["website/src/core/*"]
    },
    "lib": [
      "ESNext",
      "DOM"
    ],
    "allowSyntheticDefaultImports": true,
    "noImplicitUseStrict": true,
  },
  "exclude": [
    "node_modules"
  ]
}